  "dependencies": {
    "@swiftyapp/cryptor": "^1.0.4",
    "babel": "^6.23.0",
    "classnames": "^2.2.6",
    "electron-log": "^4.1.1",
    "electron-unhandled": "^3.0.2",
    "electron-util": "^0.14.1",
    "fs-extra": "^9.0.0",
    "generate-password": "^1.5.1",
    "googleapis": "^49.0.0",
