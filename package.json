    "acorn": "^7.1.1",
    "minimist": "^1.2.5",
    "kind-of": "^6.0.3",
    "js-yaml": "^3.13.1"
  },
  "build": {
    "appId": "com.electron.swifty",
    "fileAssociations": [
      {
        "ext": "swftx",
        "role": "None",
        "icon": "build/fileicon.icns",
        "mimeType": "application/vnd.swftx"
      }
    ],
    "publish": [
      {
        "provider": "github",
        "owner": "alchaplinsky",
        "repo": "swifty"
      }
    ],
    "win": {
      "icon": "build/icon.ico"
    },
    "linux": {
      "category": "Utilities",
      "target": [
        "deb",
        "rpm",
        "AppImage"
      ]
    },
    "mac": {
      "icon": "build/icon.icns",
      "category": "your.app.category.type"
    }
  }
}
